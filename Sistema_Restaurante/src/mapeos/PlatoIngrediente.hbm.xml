<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping >
	<class name = "datos.PlatoIngrediente" table = "platoIngrediente" >
		<id column = "idPlatoIngrediente" name = "idPlatoIngrediente" >
			<generator class = "identity" />
		</id >
		<property column = "cantidad" name = "cantidad" type = "float" />
		<many-to-one name = "plato" />	
		<many-to-one name = "ingrediente" />
	</class >		
</hibernate-mapping >


